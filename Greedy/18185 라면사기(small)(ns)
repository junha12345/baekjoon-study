# i -> 3
# i, i+1 -> 5
# i, i+1, i+2 ->7
# point = 3원짜리 라면의 수 // 최소가 되도록
import sys
input = sys.stdin.readline

n = int(input())
nlist = list(map(int, input().split()))
nums = []
print(nlist)
flag, cnt = 0, 0
for i in range(n) :
    if nlist[i] == 0 and flag == 0 :
            nums.append(nlist[cnt:i])
            cnt = i
            flag = 1
    elif nlist[i] == 0 :
        cnt = i
        continue
    else : flag = 0
nums.append(nlist[cnt+1:])
print(nums)

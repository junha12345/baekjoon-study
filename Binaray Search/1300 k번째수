import sys
# 어떤 수보다 작거나 같은수 갯수 >= k -> 어떤수를 증가
# 어떤 수보다 작거나 같은수 갯수 <= k -> 어떤수를 감소
n = int(input())
k = int(input())
start, end = 1, k
while start <= end :
    mid = (start+end)//2
    cnt = 0
    for i in range(1,n+1) :
        cnt += min(mid//i, n)
    if cnt >= k :
        ans = mid
        end = mid-1
    else :
        start = mid+1
print(ans)

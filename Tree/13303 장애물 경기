import sys
input = sys.stdin.readline

n = int(input())
a, b = map(int, input().split()) # y, max x value
nlist = []
for _ in range(n) :
    nlist.append(list(map(int, input().split())))
nlist.sort(key=lambda x: x[0])
# 이중 딕셔너리 사용 key1 = idx, key2 = y, value = distance 
# 같은 idx에서 y같이 같을때 최소의 distance를 고름
def dfs(idx, y, dist) :
    ans.setdefault(idx,[])
    if idx != n :
        
    else :
        ans.setdefault(dist,[])
        ans[dist].append(y)
key = {}
ans = []
dfs(0, a, 0)
